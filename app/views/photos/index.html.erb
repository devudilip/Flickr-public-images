<div class="page-header">
  <h1>Uploaded by all</h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Photo </th>
      <th>Name </th>
      <th>Author</th>
    </tr>
  </thead>
  <tbody>
    <% @results['items'].each do |photo| %>
    <tr>
     <td> <img src="<%= get_clean_url(photo['description'])%>.jpg " > </td>
     <td><%= link_to truncate(photo['title']), photo_path(photo['author_id']), title: photo['title'] %></td>
     <td> <%= get_author(photo['author'])%> </td>
   </tr>
   <% end %>
 </tbody>
</table>


